types:

    complex {
        fields:
            num r;
            num i;

        operations:
            complex +(complex x, complex y) {
                vars:
                    complex result;
                process:
                    result.r = x.r + y.r;
                    result.i = x.i + y.i
                    return result;
            }
    }

vars:
  complex x;


process:
  x.r = 10;
  x.i = 20;

  print("x.r: " , x.r);
  print("x.i: " , x.i);
